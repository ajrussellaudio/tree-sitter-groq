{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "groq",
      "camelcase": "Groq",
      "title": "GROQ",
      "scope": "source.groq",
      "file-types": [
        "groq"
      ],
      "injection-regex": "^groq$",
      "class-name": "TreeSitterGroq"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Query language made at Sanity.io for filtering and projecting JSON documents",
    "authors": [
      {
        "name": "Alan Russell",
        "email": "ajrussellaudio@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/ajrussellaudio/tree-sitter-groq"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}